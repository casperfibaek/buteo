    </node>
    <node id="Remove-GRD-Border-Noise">
      <displayPosition x="256.0" y="128.0"/>
    </node>
    <node id="Apply-Orbit-File">
      <displayPosition x="156.0" y="128.0"/>
    </node>
    <node id="Read">
      <displayPosition x="33.0" y="123.0"/>
    </node>
  </applicationData>
</graph>
